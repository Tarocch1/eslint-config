{
  "name": "@tarocch1/eslint-config",
  "type": "module",
  "version": "4.2.0",
  "description": "Tarocch1's eslint config.",
  "author": "Tarocch1 <huiben.fang@gmail.com> (https://github.com/Tarocch1)",
  "license": "MIT",
  "homepage": "https://github.com/Tarocch1/eslint-config#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/Tarocch1/eslint-config.git"
  },
  "bugs": "https://github.com/Tarocch1/eslint-config/issues",
  "exports": {
    ".": "./dist/index.mjs",
    "./prettier": "./dist/prettier.mjs",
    "./package.json": "./package.json"
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.mts",
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "prepack": "pnpm run build",
    "build": "tsdown",
    "build:inspector": "config-inspector build --base eslint-config/",
    "lint": "eslint --fix",
    "lint-staged": "lint-staged",
    "prepare": "husky"
  },
  "dependencies": {
    "@antfu/eslint-config": "catalog:",
    "eslint": "catalog:",
    "eslint-plugin-format": "catalog:",
    "jiti": "catalog:",
    "prettier": "catalog:",
    "prettier-plugin-lint-md": "catalog:"
  },
  "devDependencies": {
    "@eslint/config-inspector": "catalog:",
    "husky": "catalog:",
    "lint-staged": "catalog:",
    "tsdown": "catalog:",
    "typescript": "catalog:"
  },
  "lint-staged": {
    "*": "eslint --fix"
  }
}
